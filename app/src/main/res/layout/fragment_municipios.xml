<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.actividades.MunicipiosFragment">

    <ScrollView
        android:id="@+id/sViewMunicipios"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <ImageButton
                android:id="@+id/IBBojaca"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/bojaca" />

            <ImageButton
                android:id="@+id/IBElRosal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/el_rosal" />

            <ImageButton
                android:id="@+id/IBFacatativa"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/facatativa" />

            <ImageButton
                android:id="@+id/IBFunza"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/funza" />

            <ImageButton
                android:id="@+id/IBMadrid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/madrid" />

            <ImageButton
                android:id="@+id/IBMosquera"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/mosquera" />

            <ImageButton
                android:id="@+id/IBSubachoque"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/subachoque" />

            <ImageButton
                android:id="@+id/IBZipacon"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/zipacon" />
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>